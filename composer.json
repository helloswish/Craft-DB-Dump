{
    "name": "swishdigital/db-dump",
    "description": "A simple way to perform database backups in Craft CMS 3.",
    "type": "craft-plugin",
    "version": "3.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "db dump"
    ],
    "support": {
        "docs": "https://github.com/chadcrowell/db-dump/blob/master/README.md",
        "issues": "https://github.com/chadcrowell/db-dump/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Swish Digital",
            "homepage": "https://helloswish.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "swishdigital\\dbdump\\": "src/"
        }
    },
    "extra": {
        "name": "DB Dump",
        "handle": "db-dump",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/chadcrowell/db-dump/master/CHANGELOG.md",
        "class": "swishdigital\\dbdump\\DbDump"
    }
}
